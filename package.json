{
  "name": "eve-goons-waitlist",
  "version": "2.0.0",
  "description": "The ESI-enabled socket-based waitlist used by the Incursions squad in Goonfleet of Eve Online.",
  "main": "index.js",
  "scripts": {
    "start": "npm run webpack && node resources/minify.config.js && node index.js",
    "build:js": "node resources/minify.config.js",
    "scheduler": "node scheduler.js",
    "build:docker": "docker build -t waitlist .",
    "start:docker": "docker run -it -v ${PWD}:/opt/goon_waitlist -v /opt/goon_waitlist/node_modules -p 3000:3000 --rm waitlist",
    "start:dockercmp": "docker-compose up",
    "test": "jest",
    "webpack": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "webpack-watch": "cross-env NODE_ENV=development webpack --config webpack.config.js --watch --mode=development"
  },
  "keywords": [
    "eve",
    "oauth",
    "esi"
  ],
  "author": "Samuel Grant",
  "license": "GPL-3.0",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@sentry/node": "^5.24.2",
    "@sentry/tracing": "^5.24.2",
    "babel-loader": "^8.0.4",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "eve-swagger": "git+https://github.com/CodesignDev/eve-swagger-js.git",
    "express": "^4.16.4",
    "express-longpoll": "0.0.4",
    "express-session": "^1.15.6",
    "heroku-ssl-redirect": "0.0.4",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "mongodb": "3.0.5",
    "morgan": "^1.9.1",
    "nunjucks": "^3.1.6",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "passport-oauth2-refresh": "^1.1.0",
    "react": "^16.6.3",
    "react-browser-notifications": "^1.0.14",
    "react-dom": "^16.6.3",
    "req-flash": "0.0.3",
    "request": "^2.88.0",
    "serve-favicon": "^2.5.0",
    "uglify-js": "^3.4.9",
    "uglifycss": "0.0.29",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev": "^1.1.1",
    "winston": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodesignDev/eve-goons-waitlist.git"
  },
  "devDependencies": {
    "jest": "^23.6.0"
  }
}
